{
  "name": "Content Creation App",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "postinstall": "concurrently -n \"client,server\" \"npm run client-install\" \"npm run server-install\"",
    "test": "cd client && npm test",
    "client": "cd client && npm run dev",
    "client-install": "cd client && npm install",
    "server": "cd server && npm run dev",
    "server-install": "cd server && npm install",
    "start": "echo 'Starting application...' && concurrently \"npm run server\" \"npm run client\"",
    "analyze": "node server/analysis/scripts/analyze.js",
    "install-analysis": "cd server/analysis && npm install"
  },
  "author": "",
